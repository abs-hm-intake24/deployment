{
  "play_app": {
    "debian_package_path" : "/path/to/intake24/api-server/DataExportService/target/intake24-data-export_4.2.0-SNAPSHOT_all.deb",
    "name": "intake24-data-export",
    "http_port": "6401",
    "http_address": "localhost",
    "java_memory_initial": "32m",
    "java_memory_max": "320m",
    "ehcache_max_size_in_bytes": "8388608",
    "main_conf_file": "../instances/{{intake24_instance_name}}/data-export-service/application.conf",
    "extra_conf_files": [
      "../instances/{{intake24_instance_name}}/play-shared/http-secret.conf",
      "../instances/{{intake24_instance_name}}/play-shared/play.mailer.conf",
      "../instances/{{intake24_instance_name}}/play-shared/databases.conf"
    ],
    "logger_conf_file": "../instances/{{intake24_instance_name}}/data-export-service/logging.xml"
  }
}

